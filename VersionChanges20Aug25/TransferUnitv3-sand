      <script>
          var j$ = jQuery.noConflict();
     
            j$(document).ready(function() {
              
            });
            
            // events are not bound if partial refresh happens. we need to bind it again
            // hence keeping bindevents separately and call them when needed
            function editRate(ratename) {
                    openForEditFunction(ratename);
            } 
             function selectPlan() {
                    //alert('in select plan');
                    selectPlanFunction();
            }   
            
            function openForEditDiscount(discountType) {
                    openForEditDiscountFunction(discountType);
            }  
            function rowModify(index,type) {
                    rowModifyFunction(index,type);
            }
            function nextPage(){ 
                //alert('Park::'+'{!earParkingError}');
                continueMethod();
                if('{!earParkingError}' == 'false' && '{!addParkingError}' == 'false' && '{!discountError}' == 'false'){
                    $("[id$='printArea']").toggle("explode");
                    $("[id$='printArea']").prev().toggle("explode");
                    $("[id$='printPaymentPlan']").next().toggle("explode");
                }
            }
            
            $("#freebiesPage").click(function(e){
                e.preventDefault();//this will prevent the link trying to navigate to another page
                var href = $(this).attr("href");//get the href so we can navigate later
                hideClickHereLink();
                //when update has finished, navigate to the other page
               // window.location = href;
                window.open(href, '_blank');
                
            });
            
            function freeeee(amount){
               document.getElementById("j_id0:newquoteform:freeId").value = amount;
               document.getElementById("j_id0:newquoteform:freeId").style.display = "block";
               //document.getElementById("j_id0:newquoteform:freeId").disabled = true;
            }
        	function freename(name){
               document.getElementById("j_id0:newquoteform:free1Id").value = name;
               document.getElementById("j_id0:newquoteform:free1Id").style.display = "block";
               //document.getElementById("j_id0:newquoteform:freeId").disabled = true;
            }
           /* function disabledFieldf(){
                document.getElementById("j_id0:newquoteform:freeId").disabled = true;
            } */
            
          </script>
