            <script>
            var j$ = jQuery.noConflict();
     
            j$(document).ready(function() {
            });
            
            function selectQuote() {
                    //alert('in select quote');
                    selectQuoteFunction();
            } 
            function rowModify(index,type) {
                    rowModifyFunction(index,type);
            }
           </script>
            <!-- / JAVASCRIPT -->
        </body>
    </html>
</apex:page>
